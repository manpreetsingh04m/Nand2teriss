/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Nand(a=a,b=b,out=notaandb);
    Nand(a=a,b=notaandb,out=nandb);
    Nand(a=notaandb,b=b,out=nanda);
    Nand(a=nandb,b=nanda,out=out);
}